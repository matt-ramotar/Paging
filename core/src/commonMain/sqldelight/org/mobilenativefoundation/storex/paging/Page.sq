CREATE TABLE IF NOT EXISTS page (
    params TEXT PRIMARY KEY NOT NULL,
    prevKey TEXT,
    nextKey TEXT,
    extras TEXT
);

getPage:
SELECT * FROM page WHERE params = :params;


getPageItemIds:
SELECT item.id
FROM page
JOIN item ON page.params = item.pageParams
WHERE page.params = :params;

setPage:
INSERT INTO page(params, prevKey, nextKey, extras) VALUES ?;


removePage:
DELETE FROM page WHERE params = :params;